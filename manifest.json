{
  "manifest_version": 2,
  "name": "Scrum for GitHub",
  "version": "1.0.0",
  "permissions": [
    "storage"
  ],
  "description": "Chrome extension to add scrum points to issues",
  "page_action": {},
  "content_scripts": [
    {
      "matches": ["https://github.com/*/projects/*"],
      "js": ["projects.js"]
    }
  ]
}
